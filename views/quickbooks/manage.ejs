<div class="container">
<table id="vendor-table" class="table-hover table-responsive table-bordered">
	<thead>
		<th>Bill Addr</th>
		<th>Tax Identifier</th>
		<th>Vendor1099</th>
		<th>Id</th>
		<th>GivenName</th>
		<th>FamilyName</th>
		<th>DisplayName</th>
		<th>PrintOnCheckName</th>
	</thead>
	<tbody>
	</tbody>
</table>
</div> <!-- close container -->
<pre id="json-records" class="">
<% var records= JSON.stringify(vendors) %>
<%= records %>
</pre>

<script type="text/javascript">
	var $records = $('#json-records'),
    myRecords = JSON.parse($records.text());
$('#vendor-table')
.bind('dynatable:preinit',function(e,dynatable){
  console.log("bound function");
  dynatable.utility.textTransform.CamelCase = function(text){
  	return text
  	.replace(/[A-Z]/,function($1){return $1.toLowerCase()})
  }
})
.dynatable({
dataset:{
  records: myRecords
  },
  table:{defaultColumnIdStyle: 'CamelCase'}
  })
</script>